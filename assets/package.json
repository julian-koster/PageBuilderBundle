{
  "name": "@juliankoster/pagebuilderbundle",
  "description": "A pagebuilder for Symfony",
  "license": "MIT",
  "version": "0.0.1",
  "keywords": [
    "symfony pagebuilder"
  ],
  "repository": "https://github.com/julian-koster/PageBuilderBundle",
  "type": "module",
  "files": [
    "dist"
  ],
  "symfony": {
    "controllers": {
      "page-builder": {
        "main":     "dist/page_builder_controller.js",
        "enabled":  true,
        "fetch":    "eager",
        "webpackMode": "eager"
      }
    },
    "importmap": {
      "@hotwired/stimulus": "^3.0.0"
    }
  },

  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.0"
  }
}